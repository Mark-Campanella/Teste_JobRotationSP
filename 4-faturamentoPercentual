SP = 67836.43
RJ = 36678.66
MG = 29229.88
ES = 27165.48
outros = 19849.53

SOMATORIA = SP + RJ + MG + ES + outros
print('Representação SP: ', (SP/SOMATORIA) * 100 )
print('\nRepresentação RJ: ', (RJ/SOMATORIA) * 100 )
print('\nRepresentação MG: ', (MG/SOMATORIA) * 100 )
print('\nRepresentação ES: ', (ES/SOMATORIA) * 100 )
print('\nRepresentação outros: ', (outros/SOMATORIA) * 100 )